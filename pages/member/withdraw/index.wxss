page{
  background: #fff;
  padding: 20rpx 24rpx;
  box-sizing: border-box;
}
.fui-cell-group .fui-cell .fui-input.money {
    font-size: 45rpx;
}

.fui-cell.no-border::before {
    display: none;
}

.fui-cell-group .fui-cell .fui-cell-icon {
    width: auto;
    height: auto;
}

.fui-cell image {
    width: 52rpx;
    height: 52rpx;
}

.fui-cell.applyradio::after {
    content: " ";
    position: absolute;
    left: 20rpx;
    right: 20rpx;
    top: 0;
    height: 1px;
    border-top: 1px solid #ebebeb;
    color: #D9D9D9;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}
.tx_header{
  width: 100%;
  height: 360rpx;
  position: relative;
  box-sizing: border-box;
  border-radius: 15rpx;
}
.tx_header_content{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
}
.tx_header_content .price{
  font-size: 108rpx;
  font-weight: bold;
  color: #fff;
  line-height: 108rpx;
}
.tx_header_content .subtitle{
  font-size: 24rpx;
  color: #fff;
}
.dum-cell-title{
  font-size: 28rpx;
  margin-top: 10rpx;
  line-height: 80rpx;
}
.dum-cell-title small{
  font-size: 24rpx;
  margin-left: 20rpx;
  color: #ccc;
}
.dum-cell-group{
  background: #f0f0f0;
  padding: 32rpx 32rpx 32rpx;
  border-radius: 15rpx;
}
.dum-cell-subtitle{
  font-size: 24rpx;
  line-height: 70rpx;
  color: #999;
  margin-top: 10rpx;
}
.dum-cell{
  background: #fff;
  padding: 20rpx 30rpx;
  border-radius: 5rpx;
}
.dum-cell-input{
  display: flex;
}
.dum-cell-input .icon{
  line-height: 52.5rpx;
  font-size: 36rpx;
  margin-right: 5rpx;
}
.dum-cell-input .dum-input{
  line-height: 52.5rpx;
  font-size: 36rpx;
  flex: 1;
}
.text-withdraw{
  font-size: 24rpx;
  color: #f5b73d;
  line-height: 52.5rpx;
}
.dum-cell-ul{
  background: #fff;
  border-radius: 5rpx;
}
.dum-cell-list{
  position: relative;
  padding: 24rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  transition-duration: 300ms;
  transition-property: background-color;
}
.dum-cell-list:before {
    content: " ";
    position: absolute;
    left: 20rpx;
    right: 20rpx;
    top: 0;
    height: 1px;
    border-top: 1px solid #ebebeb;
    color: #D9D9D9;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}
.dum-cell-list .dum-cell-icon {
  margin-right: 12rpx;
  width: auto;
  height: auto;
  line-height: 32rpx;
  color: #666;
  text-align: center;
}
.dum-cell-list image {
  width: 52rpx;
  height: 52rpx;
}
.dum-cell-list .dum-cell-info {
  flex: 1;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 28rpx;
  color: #000;
}
.dum-cell-list .dum-cell-label {
  position: relative;
  display: block;
  width: 160rpx;
  padding: 0;
  font-size: 28rpx;
  color: #666;
}
.dum-cell-list input {
  width: 100%;
  border: 0;
  outline: 0;
  background-color: transparent;
  font-size: inherit;
  color: inherit;
  height: 1.41176471em;
  line-height: 1.41176471;
}
.dum-cell-list .dum-cell-remark {
  color: #888;
  text-align: right;
  font-size: 30rpx;
  margin-right: 8rpx;
}
.dum-cell-list .dum-cell-remark:after {
  content: " ";
  display: inline-block;
  transform: rotate(45deg);
  height: 16rpx;
  width: 16rpx;
  border-width: 1px 1px 0 0;
  border-color: #C8C8CD;
  border-style: solid;
  position: relative;
  top: -1px;
  margin-left: .3em;
}
.dum-cell-list .dum-cell-remark.noremark:after {
  display: none;
  margin-left: 0;
}
.dum-cell-hint{
  font-size: 28rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
}
.btn{
  margin: 40rpx 0;
  border-radius: 50rpx;
  font-size: 28rpx;
}
.btn.btn-danger{
  background: #fbbe60;
  border: 1rpx solid #fef6e7;
  box-shadow: 0 0 10rpx #fef6e7;
}
.btn.btn-danger.disabled{
  box-shadow: inherit;
}





